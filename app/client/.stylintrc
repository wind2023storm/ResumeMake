{
  "blocks": false,
  "brackets": "never",
  "colons": {
    "expect": "never",
    "error": true
  },
  "colors": false,
  "commaSpace": {
    "expect": "always",
    "error": true
  },
  "commentSpace": {
    "expect": "always",
    "error": true
  },
  "cssLiteral": "never",
  "depthLimit": false,
  "duplicates": false,
  "efficient": "always",
  "exclude": [],
  "extendPref": false,
  "globalDupe": false,
  "groupOutputByFile": true,
  "indentPref": 2,
  "leadingZero": false,
  "maxErrors": false,
  "maxWarnings": false,
  "mixed": true,
  "namingConvention": "lowercase-dash",
  "namingConventionStrict": true,
  "none": false,
  "noImportant": false,
  "parenSpace": {
    "expect": "never",
    "error": true
  },
  "placeholders": "always",
  "prefixVarsWithDollar": false,
  "quotePref": {
    "expect": "single",
    "error": true
  },
  "reporter": "stylint-stylish",
  "reporterOptions": {
    "columns": ["lineData", "severity", "description", "rule"],
    "columnSplitter": "  ",
    "showHeaders": false,
    "truncate": true
  },
  "semicolons": {
    "expect": "never",
    "error": true
  },
  "sortOrder": false,
  "stackedProperties": "never",
  "trailingWhitespace": {
    "expect": "never",
    "error": true
  },
  "universal": "never",
  "valid": true,
  "zeroUnits": false,
  "zIndexNormalize": false
}
