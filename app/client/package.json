{
  "name": "latexresume-client",
  "version": "0.1.0",
  "description": "A webapp for generating stunning LaTeX resumes.",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.dev.config.js",
    "test": "npm run lint --silent",
    "lint": "blyss | snazzy",
    "fix": "blyss --fix",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack --optimize-minimize -p --config webpack.prod.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/saadq/resume-generator-client"
  },
  "author": "Saad Quadri",
  "license": "MIT",
  "homepage": "https://github.com/saadq/saadq-client-boilerplate",
  "dependencies": {
    "lodash": "^4.17.4",
    "prop-types": "^15.5.8",
    "react": "15.4",
    "react-dom": "15.4",
    "react-pdf-js": "^1.0.35",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-form": "^6.6.3",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "blyss": "^3.0.2",
    "blyss-loader": "^1.0.0",
    "bulma-stylus": "^0.4.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-devtools-extension": "^2.13.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.0",
    "snazzy": "^6.0.0",
    "style-loader": "^0.16.1",
    "stylint": "^1.5.9",
    "stylint-loader": "^1.0.0",
    "stylint-stylish": "^1.4.0",
